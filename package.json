{
  "name": "sun-kexin-personal-website",
  "version": "1.0.0",
  "description": "孙可馨的个人空间 - 个性化个人展示平台",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:portfolio": "vite --config vite.portfolio.config.ts",
    "dev:admin": "vite --config vite.admin.config.ts",
    "build": "vue-tsc && vite build",
    "build:portfolio": "vue-tsc && vite build --config vite.portfolio.config.ts",
    "build:admin": "vue-tsc && vite build --config vite.admin.config.ts",
    "preview": "vite preview",
    "preview:portfolio": "vite preview --config vite.portfolio.config.ts",
    "preview:admin": "vite preview --config vite.admin.config.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:portfolio": "npm run build:portfolio && gh-pages -d dist-portfolio --repo https://github.com/你的用户名/sun-kexin-portfolio.git",
    "deploy:admin": "npm run build:admin && gh-pages -d dist-admin --repo https://github.com/你的用户名/sun-kexin-admin.git"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.8",
    "gsap": "^3.12.4",
    "leaflet": "^1.9.4",
    "pinia": "^2.1.7",
    "tdesign-vue-next": "^1.9.0",
    "three": "^0.160.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/three": "^0.160.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "gh-pages": "^6.3.0",
    "json-server": "^1.0.0-beta.3",
    "json-server-auth": "^2.1.0",
    "prettier": "^3.1.1",
    "sass": "^1.70.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  },
  "type": "module"
}
